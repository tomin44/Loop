// For loop
console.log("Example 1: For loop");
for (let i = 1; i <= 5; i++) {
  console.log("Iteration " + i);
}

// 2: While loop
console.log("Example 2: While loop");
let count = 1;
while (count <= 5) {
  console.log("Iteration " + count);
  count++;
}

//  Do-While loop
console.log("Example 3: Do-While loop");
let num = 1;
do {
  console.log("Iteration " + num);
  num++;
} while (num <= 15);
